	(ownedComment += Comment ( ',' ownedComment += Comment)*)?value = STRING '}';

RecordValue :
	{RecordValue} 'RecordValue''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedFieldValue += BasicPropertyAssociation ( ',' ownedFieldValue += BasicPropertyAssociation)*)?'}';

AbstractNamedValue :
	Property | EnumerationLiteral | PropertyConstant;

FeatureGroupPrototypeBinding :
	{FeatureGroupPrototypeBinding} 'FeatureGroupPrototypeBinding''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?formal =[Prototype|IDDT]
	actual = FeatureGroupPrototypeActual '}';

FeaturePrototypeBinding :
	{FeaturePrototypeBinding} 'FeaturePrototypeBinding''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?formal =[Prototype|IDDT]
	actual = FeaturePrototypeActual '}';

ComponentPrototypeBinding :
	{ComponentPrototypeBinding} 'ComponentPrototypeBinding''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?formal =[Prototype|IDDT]
	(actual += ComponentPrototypeActual ( ',' actual += ComponentPrototypeActual)*) '}';

ComponentPrototypeActual :
	{ComponentPrototypeActual} 'ComponentPrototypeActual' 'category'  ':' category = ComponentCategory '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(binding += PrototypeBinding ( ',' binding += PrototypeBinding)*)?subcomponentType =[SubcomponentType|IDDT] '}';

FeaturePrototypeActual :
	PortSpecification | FeaturePrototypeReference | FeatureGroupPrototypeActual | AccessSpecification;

PortSpecification :
	{PortSpecification} 'PortSpecification'in = BOOLEAN out = BOOLEAN 'direction'  ':' direction = DirectionType '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?category = PortCategory classifier =[ComponentClassifier|IDDT]?componentPrototype =[ComponentPrototype|IDDT]?'}';

FeaturePrototypeReference :
	{FeaturePrototypeReference} 'FeaturePrototypeReference'in = BOOLEAN out = BOOLEAN 'direction'  ':' direction = DirectionType '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?prototype =[FeaturePrototype|IDDT] '}';

AccessSpecification :
	{AccessSpecification} 'AccessSpecification' 'kind'  ':' kind = AccessType '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?category = AccessCategory classifier =[ComponentClassifier|IDDT]?componentPrototype =[ComponentPrototype|IDDT]?'}';

FeatureGroupPrototypeActual :
	{FeatureGroupPrototypeActual} 'FeatureGroupPrototypeActual''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? (binding += PrototypeBinding ( ',' binding += PrototypeBinding)*)?featureType =[FeatureType|IDDT] '}';

DirectedRelationship :
	Generalization;

ArraySizeProperty :
	PropertyConstant;

IDDT:
	BOOLEAN | INTEGER | ID;

terminal ID : '^'? ('a'..'z' | 'A'..'Z' | '_') ('a'..'z' | 'A'..'Z' | '_' | '0'..'9')*;
