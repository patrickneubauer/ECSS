grammar opL

import "http://aadl.info/AADL/2.0"
import "http://www.eclipse.org/emf/2002/Ecore" as ecore


Element :
	ConnectedElement | ArrayRange | ArrayDimension | ContainedNamedElement | ModeBinding | ArraySize |
	PropertyAssociation | PropertyOwner | ContainmentPathElement | NamedElement | FlowEnd |
	ComponentImplementationReference | BasicPropertyAssociation | FlowSegment | ModeTransitionTrigger | Comment |
	NumericRange | Relationship | PrototypeBinding | EndToEndFlowSegment | ArrayableElement | PropertyExpression;

ModeBinding :
	{ModeBinding} 'ModeBinding''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?parentMode =[Mode|IDDT] derivedMode =[Mode|IDDT]?'}';

NumericRange :
	{NumericRange} 'NumericRange''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?upperBound = PropertyExpression lowerBound = PropertyExpression '}';

FlowSegment :
	{FlowSegment} 'FlowSegment''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?flowElement =[FlowElement|IDDT] context =[Context|IDDT]?'}';

PropertyOwner :
	MetaclassReference | ClassifierValue;

BasicPropertyAssociation :
	{BasicPropertyAssociation} 'BasicPropertyAssociation''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?property =[BasicProperty|IDDT] ownedValue = PropertyExpression '}';

ArraySize :
	{ArraySize} 'ArraySize''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?size = INTEGER sizeProperty =[ArraySizeProperty|IDDT]?'}';

ArrayableElement :
	Feature | FeaturePrototypeActual | Subcomponent | ComponentPrototypeActual;

ArrayDimension :
	{ArrayDimension} 'ArrayDimension''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?size = ArraySize ?'}';

PropertyAssociation :
	{PropertyAssociation} 'PropertyAssociation' 'append'  ':' append = BOOLEAN  'constant'  ':' constant = BOOLEAN '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?property =[Property|IDDT] (appliesTo += ContainedNamedElement ( ',' appliesTo += ContainedNamedElement)*)? (inBinding +=[Classifier|IDDT] ( ',' inBinding +=[Classifier|IDDT])*)?
	(ownedValue += ModalPropertyValue ( ',' ownedValue += ModalPropertyValue)*) '}';

Relationship :
	DirectedRelationship;

ContainedNamedElement :
	ReferenceValue;

PrototypeBinding :
	FeatureGroupPrototypeBinding | ComponentPrototypeBinding | FeaturePrototypeBinding;

ContainmentPathElement :
	{ContainmentPathElement} 'ContainmentPathElement''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)? (arrayRange += ArrayRange
	( ',' arrayRange += ArrayRange)*)?namedElement =[NamedElement|IDDT] annexName = STRING ?path = ContainmentPathElement ?'}';

PropertyExpression :
	PropertyValue | Operation | ListValue;

NamedElement :
	System | Process | FeatureGroupTypeRename | TypedElement | ModalElement | VirtualProcessor | Device | Processor |
	Namespace | EndToEndFlowElement | Context | ConnectionEnd | SubprogramGroup | ComponentTypeRename | ModelUnit |
	PackageRename | Data | Subprogram | Memory | Abstract | ClassifierFeature | Thread | TriggerPort | EnumerationLiteral | ThreadGroup | VirtualBus | Bus | Type | RefinableElement | Flow | AnnexLibrary;

ConnectedElement :
	{ConnectedElement} 'ConnectedElement''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?context =[Context|IDDT]?connectionEnd =[ConnectionEnd|IDDT]
	next = ConnectedElement ?'}';

ModeTransitionTrigger :
	{ModeTransitionTrigger} 'ModeTransitionTrigger''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?context =[Context|IDDT]?triggerPort =[TriggerPort|IDDT] '}';

Comment :
	{Comment} 'Comment''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?body = STRING ?'}';

EndToEndFlowSegment :
	{EndToEndFlowSegment} 'EndToEndFlowSegment''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?flowElement =[EndToEndFlowElement|IDDT]
	context =[Context|IDDT]?'}';

ArrayRange :
	{ArrayRange} 'ArrayRange''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?lowerBound = INTEGER upperBound = INTEGER '}';

ComponentImplementationReference :
	{ComponentImplementationReference} 'ComponentImplementationReference''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?implementation =[ComponentImplementation|IDDT] (ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?'}';

FlowEnd :
	{FlowEnd} 'FlowEnd''{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?context =[Context|IDDT]?feature =[Feature|IDDT] '}';

Feature :
	Access | DirectedFeature;

DirectedFeature :
	FeatureGroup | AbstractFeature | Port | Parameter;

Access :
	SubprogramAccess | SubprogramGroupAccess | DataAccess | BusAccess;

SubprogramGroupAccess :
	{SubprogramGroupAccess} '!SubprogramGroupAccess' 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?
	(featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?prototype =[ComponentPrototype|IDDT]?refined =[Feature|IDDT]?classifier =[Classifier|IDDT]?kind = AccessType category = AccessCategory subprogramGroupFeatureClassifier =[SubprogramGroupSubcomponentType|IDDT]?'}';

DataAccess :
	{DataAccess} '!DataAccess' 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?
	(featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?prototype =[ComponentPrototype|IDDT]?refined =[Feature|IDDT]?classifier =[Classifier|IDDT]?kind = AccessType category = AccessCategory dataFeatureClassifier =[DataSubcomponentType|IDDT]?'}';

SubprogramAccess :
	{SubprogramAccess} '!SubprogramAccess' 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?
	(featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?prototype =[ComponentPrototype|IDDT]?featureClassifier =[FeatureClassifier|IDDT]?refined =[Feature|IDDT]?classifier =[Classifier|IDDT]?kind = AccessType category = AccessCategory subprogramFeatureClassifier =[SubprogramSubcomponentType|IDDT]?'}';

BusAccess :
	{BusAccess} '!BusAccess' 'virtual'  ':' virtual = BOOLEAN  'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT]
	( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?prototype =[ComponentPrototype|IDDT]?refined =[Feature|IDDT]?classifier =[Classifier|IDDT]?kind = AccessType category = AccessCategory busFeatureClassifier =[BusFeatureClassifier|IDDT]?'}';

BusFeatureClassifier :
	BusSubcomponentType | VirtualBusSubcomponentType;

BusSubcomponentType :
	AbstractClassifier | AbstractPrototype | BusPrototype | BusClassifier;

VirtualBusSubcomponentType :
	AbstractClassifier | AbstractPrototype | VirtualBusClassifier | VirtualBusPrototype;

VirtualBusPrototype :
	{VirtualBusPrototype} '!VirtualBusPrototype' 'array'  ':' array = BOOLEAN  'name'  ':' name = IDDT '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?refinementContext =[Classifier|IDDT]?refinedElement =[RefinableElement|IDDT]? (featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?refined =[Prototype|IDDT]?constrainingClassifier =[ComponentClassifier|IDDT]?'}';

AbstractClassifier :
	AbstractType | AbstractImplementation;

VirtualBusClassifier :
	VirtualBusImplementation | VirtualBusType;

AbstractPrototype :
	{AbstractPrototype} '!AbstractPrototype' 'array'  ':' array = BOOLEAN  'name'  ':' name = IDDT '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?refinementContext =[Classifier|IDDT]?refinedElement =[RefinableElement|IDDT]? (featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?refined =[Prototype|IDDT]?constrainingClassifier =[ComponentClassifier|IDDT]?'}';

VirtualBusImplementation :
	{VirtualBusImplementation} '!VirtualBusImplementation'noPrototypes = BOOLEAN noAnnexes = BOOLEAN noProperties = BOOLEAN derivedModes = BOOLEAN noFlows = BOOLEAN noModes = BOOLEAN noSubcomponents = BOOLEAN noConnections = BOOLEAN noCalls = BOOLEAN 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?
	(member +=[NamedElement|IDDT] ( ',' member +=[NamedElement|IDDT])*)? (inheritedMember +=[NamedElement|IDDT] ( ',' inheritedMember +=[NamedElement|IDDT])*)? (general +=[Classifier|IDDT] ( ',' general +=[Classifier|IDDT])*)?
	(ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? (ownedMode += Mode ( ',' ownedMode += Mode)*)? (ownedModeTransition += ModeTransition ( ',' ownedModeTransition += ModeTransition)*)?type =[ComponentType|IDDT] extended =[ComponentImplementation|IDDT]? (ownedFlowImplementation += FlowImplementation
	( ',' ownedFlowImplementation += FlowImplementation)*)? (ownedConnection +=[Connection|IDDT] ( ',' ownedConnection +=[Connection|IDDT])*)?ownedExtension = ImplementationExtension ?ownedRealization = Realization (ownedEndToEndFlow += EndToEndFlow ( ',' ownedEndToEndFlow += EndToEndFlow)*)?
	(ownedAbstractSubcomponent += AbstractSubcomponent ( ',' ownedAbstractSubcomponent += AbstractSubcomponent)*)?
	(ownedAccessConnection += AccessConnection ( ',' ownedAccessConnection += AccessConnection)*)?
	(ownedParameterConnection += ParameterConnection ( ',' ownedParameterConnection += ParameterConnection)*)?
	(ownedPortConnection += PortConnection ( ',' ownedPortConnection += PortConnection)*)?
	(ownedFeatureConnection += FeatureConnection ( ',' ownedFeatureConnection += FeatureConnection)*)?
	(ownedFeatureGroupConnection += FeatureGroupConnection ( ',' ownedFeatureGroupConnection += FeatureGroupConnection)*)? (ownedProcessorFeature +=[ProcessorFeature|IDDT] ( ',' ownedProcessorFeature +=[ProcessorFeature|IDDT])*)? (ownedInternalFeature +=[InternalFeature|IDDT] ( ',' ownedInternalFeature +=[InternalFeature|IDDT])*)? (ownedEventSource += EventSource ( ',' ownedEventSource += EventSource)*)? (ownedEventDataSource += EventDataSource ( ',' ownedEventDataSource += EventDataSource)*)? (ownedPortProxy += PortProxy ( ',' ownedPortProxy += PortProxy)*)?
	(ownedSubprogramProxy += SubprogramProxy ( ',' ownedSubprogramProxy += SubprogramProxy)*)?
	(ownedVirtualBusSubcomponent += VirtualBusSubcomponent ( ',' ownedVirtualBusSubcomponent += VirtualBusSubcomponent)*)?'}';

VirtualBusType :
	{VirtualBusType} '!VirtualBusType' 'noPrototypes'  ':' noPrototypes = BOOLEAN  'noAnnexes'  ':' noAnnexes = BOOLEAN  'noProperties'  ':' noProperties = BOOLEAN  'derivedModes'  ':' derivedModes = BOOLEAN  'noFlows'  ':' noFlows = BOOLEAN  'noModes'  ':' noModes = BOOLEAN  'noFeatures'  ':' noFeatures = BOOLEAN  'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (member +=[NamedElement|IDDT] ( ',' member +=[NamedElement|IDDT])*)? (inheritedMember +=[NamedElement|IDDT] ( ',' inheritedMember +=[NamedElement|IDDT])*)?
	(general +=[Classifier|IDDT] ( ',' general +=[Classifier|IDDT])*)? (ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? (ownedMode += Mode ( ',' ownedMode += Mode)*)?
	(ownedModeTransition += ModeTransition ( ',' ownedModeTransition += ModeTransition)*)? (ownedFeature +=[Feature|IDDT]
	( ',' ownedFeature +=[Feature|IDDT])*)?extended =[ComponentType|IDDT]? (ownedFlowSpecification += FlowSpecification
	( ',' ownedFlowSpecification += FlowSpecification)*)?ownedExtension = TypeExtension ?
	(ownedFeatureGroup += FeatureGroup ( ',' ownedFeatureGroup += FeatureGroup)*)?
	(ownedAbstractFeature += AbstractFeature ( ',' ownedAbstractFeature += AbstractFeature)*)? (ownedDataPort += DataPort
	( ',' ownedDataPort += DataPort)*)? (ownedEventDataPort += EventDataPort ( ',' ownedEventDataPort += EventDataPort)*)? (ownedEventPort += EventPort ( ',' ownedEventPort += EventPort)*)?
	(ownedBusAccess += BusAccess ( ',' ownedBusAccess += BusAccess)*)?'}';

DataPort :
	{DataPort} '!DataPort' 'in'  ':' in = BOOLEAN  'out'  ':' out = BOOLEAN  'name'  ':' name = IDDT '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?prototype =[ComponentPrototype|IDDT]?refined =[Feature|IDDT]?classifier =[Classifier|IDDT]?direction = DirectionType category = PortCategory dataFeatureClassifier =[DataSubcomponentType|IDDT]?'}';

enum PortCategory:
data | event | eventData ;

DataSubcomponentType :
	AbstractClassifier | DataClassifier | AbstractPrototype | DataPrototype;

DataClassifier :
	DataImplementation | DataType;

DataPrototype :
	{DataPrototype} '!DataPrototype' 'array'  ':' array = BOOLEAN  'name'  ':' name = IDDT '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?refinementContext =[Classifier|IDDT]?refinedElement =[RefinableElement|IDDT]? (featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?refined =[Prototype|IDDT]?constrainingClassifier =[ComponentClassifier|IDDT]?'}';

DataImplementation :
	{DataImplementation} '!DataImplementation'noPrototypes = BOOLEAN noAnnexes = BOOLEAN noProperties = BOOLEAN derivedModes = BOOLEAN noFlows = BOOLEAN noModes = BOOLEAN noSubcomponents = BOOLEAN noConnections = BOOLEAN noCalls = BOOLEAN 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)?
	(ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (member +=[NamedElement|IDDT]
	( ',' member +=[NamedElement|IDDT])*)? (inheritedMember +=[NamedElement|IDDT] ( ',' inheritedMember +=[NamedElement|IDDT])*)? (general +=[Classifier|IDDT] ( ',' general +=[Classifier|IDDT])*)?
	(ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? (ownedMode += Mode ( ',' ownedMode += Mode)*)? (ownedModeTransition += ModeTransition ( ',' ownedModeTransition += ModeTransition)*)?type =[ComponentType|IDDT] extended =[ComponentImplementation|IDDT]? (ownedFlowImplementation += FlowImplementation
	( ',' ownedFlowImplementation += FlowImplementation)*)? (ownedConnection +=[Connection|IDDT] ( ',' ownedConnection +=[Connection|IDDT])*)?ownedExtension = ImplementationExtension ?ownedRealization = Realization (ownedEndToEndFlow += EndToEndFlow ( ',' ownedEndToEndFlow += EndToEndFlow)*)?
	(ownedAbstractSubcomponent += AbstractSubcomponent ( ',' ownedAbstractSubcomponent += AbstractSubcomponent)*)?
	(ownedAccessConnection += AccessConnection ( ',' ownedAccessConnection += AccessConnection)*)?
	(ownedParameterConnection += ParameterConnection ( ',' ownedParameterConnection += ParameterConnection)*)?
	(ownedPortConnection += PortConnection ( ',' ownedPortConnection += PortConnection)*)?
	(ownedFeatureConnection += FeatureConnection ( ',' ownedFeatureConnection += FeatureConnection)*)?
	(ownedFeatureGroupConnection += FeatureGroupConnection ( ',' ownedFeatureGroupConnection += FeatureGroupConnection)*)? (ownedEventSource += EventSource ( ',' ownedEventSource += EventSource)*)? (ownedEventDataSource += EventDataSource ( ',' ownedEventDataSource += EventDataSource)*)? (ownedPortProxy += PortProxy ( ',' ownedPortProxy += PortProxy)*)?
	(ownedSubprogramProxy += SubprogramProxy ( ',' ownedSubprogramProxy += SubprogramProxy)*)?
	(ownedDataSubcomponent += DataSubcomponent ( ',' ownedDataSubcomponent += DataSubcomponent)*)?
	(ownedSubprogramSubcomponent += SubprogramSubcomponent ( ',' ownedSubprogramSubcomponent += SubprogramSubcomponent)*)?'}';

DataType :
	{DataType} '!DataType' 'noPrototypes'  ':' noPrototypes = BOOLEAN  'noAnnexes'  ':' noAnnexes = BOOLEAN  'noProperties'  ':' noProperties = BOOLEAN  'derivedModes'  ':' derivedModes = BOOLEAN  'noFlows'  ':' noFlows = BOOLEAN  'noModes'  ':' noModes = BOOLEAN  'noFeatures'  ':' noFeatures = BOOLEAN  'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (member +=[NamedElement|IDDT] ( ',' member +=[NamedElement|IDDT])*)? (inheritedMember +=[NamedElement|IDDT] ( ',' inheritedMember +=[NamedElement|IDDT])*)?
	(general +=[Classifier|IDDT] ( ',' general +=[Classifier|IDDT])*)? (ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? (ownedMode += Mode ( ',' ownedMode += Mode)*)?
	(ownedModeTransition += ModeTransition ( ',' ownedModeTransition += ModeTransition)*)? (ownedFeature +=[Feature|IDDT]
	( ',' ownedFeature +=[Feature|IDDT])*)?extended =[ComponentType|IDDT]? (ownedFlowSpecification += FlowSpecification
	( ',' ownedFlowSpecification += FlowSpecification)*)?ownedExtension = TypeExtension ?
	(ownedFeatureGroup += FeatureGroup ( ',' ownedFeatureGroup += FeatureGroup)*)?
	(ownedAbstractFeature += AbstractFeature ( ',' ownedAbstractFeature += AbstractFeature)*)?
	(ownedSubprogramAccess += SubprogramAccess ( ',' ownedSubprogramAccess += SubprogramAccess)*)?
	(ownedDataAccess += DataAccess ( ',' ownedDataAccess += DataAccess)*)?
	(ownedSubprogramGroupAccess += SubprogramGroupAccess ( ',' ownedSubprogramGroupAccess += SubprogramGroupAccess)*)?'}';

PortProxy :
	{PortProxy} '!PortProxy' 'in'  ':' in = BOOLEAN  'out'  ':' out = BOOLEAN  'name'  ':' name = IDDT '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?refinedElement =[RefinableElement|IDDT]?
	(featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?direction = DirectionType dataClassifier =[DataClassifier|IDDT]?'}';

enum DirectionType:
in | out | inOut ;

RefinableElement :
	StructuralFeature;

StructuralFeature :
	Connection | Feature | ProcessorFeature | Subcomponent | InternalFeature | Prototype | FlowFeature;

Subcomponent :
	BusSubcomponent | MemorySubcomponent | DeviceSubcomponent | DataSubcomponent | SystemSubcomponent |
	ThreadGroupSubcomponent | SubprogramGroupSubcomponent | AbstractSubcomponent | ThreadSubcomponent |
	ProcessSubcomponent | SubprogramSubcomponent | VirtualBusSubcomponent | ProcessorSubcomponent |
	VirtualProcessorSubcomponent;

InternalFeature :
	EventDataSource | EventSource;

Connection :
	AccessConnection | PortConnection | FeatureConnection | ParameterConnection | FeatureGroupConnection;

ProcessorFeature :
	PortProxy | SubprogramProxy;

FlowFeature :
	EndToEndFlow | FlowSpecification;

Prototype :
	ComponentPrototype | FeatureGroupPrototype | FeaturePrototype;

FeatureGroupPrototype :
	{FeatureGroupPrototype} '!FeatureGroupPrototype' 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?
	(featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?refined =[Prototype|IDDT]?constrainingFeatureGroupType =[FeatureGroupType|IDDT]?'}';

ComponentPrototype :
	SystemPrototype | SubprogramPrototype | SubprogramGroupPrototype | ProcessPrototype | ThreadPrototype |
	AbstractPrototype | DataPrototype | MemoryPrototype | VirtualBusPrototype | ProcessorPrototype |
	VirtualProcessorPrototype | DevicePrototype | BusPrototype | ThreadGroupPrototype;

FeaturePrototype :
	{FeaturePrototype} '!FeaturePrototype' 'in'  ':' in = BOOLEAN  'out'  ':' out = BOOLEAN  'name'  ':' name = IDDT '{'
	(ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (featuringClassifier +=[Classifier|IDDT] ( ',' featuringClassifier +=[Classifier|IDDT])*)?refined =[Prototype|IDDT]?direction = DirectionType constrainingClassifier =[ComponentClassifier|IDDT]?'}';

terminal BOOLEAN returns Boolean:
'BOOLEAN' /*Don't know how to handle BOOLEAN!*/
;

ComponentClassifier :
	ComponentType | MemoryClassifier | ThreadGroupClassifier | ProcessorClassifier | ComponentImplementation |
	SystemClassifier | VirtualBusClassifier | ThreadClassifier | ProcessClassifier | AbstractClassifier | DataClassifier | DeviceClassifier | SubprogramGroupClassifier | VirtualProcessorClassifier | SubprogramClassifier | BusClassifier;

ThreadGroupClassifier :
	ThreadGroupType | ThreadGroupImplementation;

VirtualProcessorClassifier :
	VirtualProcessorType | VirtualProcessorImplementation;

ProcessClassifier :
	ProcessType | ProcessImplementation;

SubprogramGroupClassifier :
	SubprogramGroupImplementation | SubprogramGroupType;

MemoryClassifier :
	MemoryImplementation | MemoryType;

SystemClassifier :
	SystemImplementation | SystemType;

ComponentType :
	SubprogramType | BusType | ProcessType | MemoryType | DataType | SubprogramGroupType | VirtualBusType | ThreadType |
	VirtualProcessorType | DeviceType | ProcessorType | ThreadGroupType | AbstractType | SystemType;

ComponentImplementation :
	DeviceImplementation | BehavioredImplementation | SystemImplementation | ProcessorImplementation |
	SubprogramGroupImplementation | BusImplementation | MemoryImplementation | VirtualBusImplementation |
	VirtualProcessorImplementation | ProcessImplementation | DataImplementation | ThreadGroupImplementation;

BusClassifier :
	BusType | BusImplementation;

ThreadClassifier :
	ThreadType | ThreadImplementation;

DeviceClassifier :
	DeviceImplementation | DeviceType;

ProcessorClassifier :
	ProcessorImplementation | ProcessorType;

SubprogramClassifier :
	SubprogramType | SubprogramImplementation;

SubprogramImplementation :
	{SubprogramImplementation} '!SubprogramImplementation'noPrototypes = BOOLEAN noAnnexes = BOOLEAN noProperties = BOOLEAN derivedModes = BOOLEAN noFlows = BOOLEAN noModes = BOOLEAN noSubcomponents = BOOLEAN noConnections = BOOLEAN noCalls = BOOLEAN 'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ?
	(member +=[NamedElement|IDDT] ( ',' member +=[NamedElement|IDDT])*)? (inheritedMember +=[NamedElement|IDDT] ( ',' inheritedMember +=[NamedElement|IDDT])*)? (general +=[Classifier|IDDT] ( ',' general +=[Classifier|IDDT])*)?
	(ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? (ownedMode += Mode ( ',' ownedMode += Mode)*)? (ownedModeTransition += ModeTransition ( ',' ownedModeTransition += ModeTransition)*)?type =[ComponentType|IDDT] extended =[ComponentImplementation|IDDT]? (ownedFlowImplementation += FlowImplementation
	( ',' ownedFlowImplementation += FlowImplementation)*)? (ownedConnection +=[Connection|IDDT] ( ',' ownedConnection +=[Connection|IDDT])*)?ownedExtension = ImplementationExtension ?ownedRealization = Realization (ownedEndToEndFlow += EndToEndFlow ( ',' ownedEndToEndFlow += EndToEndFlow)*)?
	(ownedAbstractSubcomponent += AbstractSubcomponent ( ',' ownedAbstractSubcomponent += AbstractSubcomponent)*)?
	(ownedAccessConnection += AccessConnection ( ',' ownedAccessConnection += AccessConnection)*)?
	(ownedParameterConnection += ParameterConnection ( ',' ownedParameterConnection += ParameterConnection)*)?
	(ownedPortConnection += PortConnection ( ',' ownedPortConnection += PortConnection)*)?
	(ownedFeatureConnection += FeatureConnection ( ',' ownedFeatureConnection += FeatureConnection)*)?
	(ownedFeatureGroupConnection += FeatureGroupConnection ( ',' ownedFeatureGroupConnection += FeatureGroupConnection)*)? (ownedInternalFeature +=[InternalFeature|IDDT] ( ',' ownedInternalFeature +=[InternalFeature|IDDT])*)? (ownedEventSource += EventSource ( ',' ownedEventSource += EventSource)*)? (ownedEventDataSource += EventDataSource ( ',' ownedEventDataSource += EventDataSource)*)? (ownedPortProxy += PortProxy ( ',' ownedPortProxy += PortProxy)*)?
	(ownedSubprogramProxy += SubprogramProxy ( ',' ownedSubprogramProxy += SubprogramProxy)*)?
	(subprogramCall +=[SubprogramCall|IDDT] ( ',' subprogramCall +=[SubprogramCall|IDDT])*)?
	(ownedSubprogramCallSequence += SubprogramCallSequence ( ',' ownedSubprogramCallSequence += SubprogramCallSequence)*)? (ownedDataSubcomponent += DataSubcomponent ( ',' ownedDataSubcomponent += DataSubcomponent)*)? (ownedSubprogramSubcomponent += SubprogramSubcomponent ( ',' ownedSubprogramSubcomponent += SubprogramSubcomponent)*)?'}';

SubprogramType :
	{SubprogramType} '!SubprogramType' 'noPrototypes'  ':' noPrototypes = BOOLEAN  'noAnnexes'  ':' noAnnexes = BOOLEAN  'noProperties'  ':' noProperties = BOOLEAN  'derivedModes'  ':' derivedModes = BOOLEAN  'noFlows'  ':' noFlows = BOOLEAN  'noModes'  ':' noModes = BOOLEAN  'noFeatures'  ':' noFeatures = BOOLEAN  'name'  ':' name = IDDT '{' (ownedElement +=[Element|IDDT] ( ',' ownedElement +=[Element|IDDT])*)? (ownedComment += Comment ( ',' ownedComment += Comment)*)?qualifiedName = STRING ? (member +=[NamedElement|IDDT] ( ',' member +=[NamedElement|IDDT])*)? (inheritedMember +=[NamedElement|IDDT] ( ',' inheritedMember +=[NamedElement|IDDT])*)?
	(general +=[Classifier|IDDT] ( ',' general +=[Classifier|IDDT])*)? (ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? (ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? (ownedMode += Mode ( ',' ownedMode += Mode)*)?
	(ownedModeTransition += ModeTransition ( ',' ownedModeTransition += ModeTransition)*)? (ownedFeature +=[Feature|IDDT]
	( ',' ownedFeature +=[Feature|IDDT])*)?extended =[ComponentType|IDDT]? (ownedFlowSpecification += FlowSpecification
