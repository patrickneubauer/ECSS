grammar opL

import "http://aadl.info/AADL/2.0"
import "http://www.eclipse.org/emf/2002/Ecore" as ecore


Element :
	NumericRange | ArrayDimension | BasicPropertyAssociation | ArrayableElement | ArrayRange | NamedElement | FlowEnd |
	ArraySize | PrototypeBinding | Comment | ConnectedElement | ModeTransitionTrigger | PropertyOwner |
	PropertyAssociation | Relationship | ContainmentPathElement | PropertyExpression | ModeBinding |
	ComponentImplementationReference | ContainedNamedElement | FlowSegment | EndToEndFlowSegment;

ConnectedElement :
	{ConnectedElement} 'connectedelement''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?context =[Context|IDDT]?connectionEnd =[ConnectionEnd|IDDT] next = ConnectedElement ?) '}?';

ComponentImplementationReference :
	{ComponentImplementationReference} 'componentimplementationreference''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?implementation =[ComponentImplementation|IDDT] (ownedPrototypeBinding += PrototypeBinding
	( ',' ownedPrototypeBinding += PrototypeBinding)*)?) '}?';

BasicPropertyAssociation :
	{BasicPropertyAssociation} 'basicpropertyassociation''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?property =[BasicProperty|IDDT] ownedValue = PropertyExpression ) '}?';

Relationship :
	DirectedRelationship;

PrototypeBinding :
	ComponentPrototypeBinding | FeatureGroupPrototypeBinding | FeaturePrototypeBinding;

FlowSegment :
	{FlowSegment} 'flowsegment''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	flowElement =[FlowElement|IDDT] & context =[Context|IDDT]?) '}?';

FlowEnd :
	{FlowEnd} 'flowend''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & context =[Context|IDDT]? &
	feature =[Feature|IDDT]) '}?';

ArraySize :
	{ArraySize} 'arraysize''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?size = INTEGER sizeProperty =[ArraySizeProperty|IDDT]?) '}?';

ContainedNamedElement :
	ReferenceValue;

ModeBinding :
	{ModeBinding} 'modebinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?parentMode =[Mode|IDDT]
	derivedMode =[Mode|IDDT]?) '}?';

NumericRange :
	{NumericRange} 'numericrange''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	upperBound = PropertyExpression & lowerBound = PropertyExpression ) '}?';

ArrayableElement :
	Feature | Subcomponent | ComponentPrototypeActual | FeaturePrototypeActual;

ContainmentPathElement :
	{ContainmentPathElement} 'containmentpathelement''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(arrayRange += ArrayRange ( ',' arrayRange += ArrayRange)*)?namedElement =[NamedElement|IDDT] annexName = STRING ?path = ContainmentPathElement ?) '}?';

ArrayDimension :
	{ArrayDimension} 'arraydimension''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	size = ArraySize ?) '}?';

PropertyAssociation :
	{PropertyAssociation} 'propertyassociation'append = BOOLEAN constant = BOOLEAN '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?property =[Property|IDDT] (appliesTo += ContainedNamedElement ( ',' appliesTo += ContainedNamedElement)*)? (inBinding +=[Classifier|IDDT] ( ',' inBinding +=[Classifier|IDDT])*)?
	(ownedValue += ModalPropertyValue ( ',' ownedValue += ModalPropertyValue)*)) '}?';

ModeTransitionTrigger :
	{ModeTransitionTrigger} 'modetransitiontrigger''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?context =[Context|IDDT]?triggerPort =[TriggerPort|IDDT]) '}?';

Comment :
	{Comment} 'comment''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & body = STRING ?) '}?';

PropertyOwner :
	MetaclassReference | ClassifierValue;

EndToEndFlowSegment :
	{EndToEndFlowSegment} 'endtoendflowsegment''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	flowElement =[EndToEndFlowElement|IDDT] & context =[Context|IDDT]?) '}?';

PropertyExpression :
	PropertyValue | Operation | ListValue;

ArrayRange :
	{ArrayRange} 'arrayrange''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & lowerBound = INTEGER &
	upperBound = INTEGER ) '}?';

NamedElement :
	Process | Device | Subprogram | FeatureGroupTypeRename | ModelUnit | Type | ThreadGroup | PackageRename |
	VirtualProcessor | System | Memory | TypedElement | TriggerPort | AnnexLibrary | ClassifierFeature | Context |
	ConnectionEnd | EnumerationLiteral | Data | Flow | Bus | Namespace | SubprogramGroup | Thread | EndToEndFlowElement |
	Abstract | ComponentTypeRename | ModalElement | RefinableElement | Processor | VirtualBus;

TypedElement :
	BasicProperty | PropertyConstant;

ConnectionEnd :
	FeatureConnectionEnd | FeatureGroupConnectionEnd | PortConnectionEnd | ParameterConnectionEnd | AccessConnectionEnd;

ClassifierFeature :
	ModeFeature | FlowImplementation | StructuralFeature | BehavioralFeature;

Process :
	ProcessPrototype | ProcessClassifier | ProcessSubcomponent;

ModelUnit :
	AadlPackage | PropertySet;

EnumerationLiteral :
	UnitLiteral;

Type :
	SubcomponentType | PropertyType | Classifier;

PackageRename :
	{PackageRename} 'packagerename'renameAll = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?renamedPackage =[AadlPackage|IDDT]) '}?';

System :
	SystemClassifier | SystemSubcomponent | SystemPrototype;

Subprogram :
	SubprogramPrototype | SubprogramSubcomponent | SubprogramClassifier;

VirtualProcessor :
	VirtualProcessorClassifier | VirtualProcessorSubcomponent | VirtualProcessorPrototype;

SubprogramGroup :
	SubprogramGroupClassifier | SubprogramGroupPrototype | SubprogramGroupSubcomponent;

ThreadGroup :
	ThreadGroupClassifier | ThreadGroupPrototype | ThreadGroupSubcomponent;

Abstract :
	AbstractClassifier | AbstractPrototype | AbstractSubcomponent;

FeatureGroupTypeRename :
	{FeatureGroupTypeRename} 'featuregrouptyperename'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & renamedFeatureGroupType =[FeatureGroupType|IDDT]) '}?';

Data :
	DataPrototype | DataSubcomponent | DataClassifier;

Device :
	DevicePrototype | DeviceClassifier | DeviceSubcomponent;

EndToEndFlowElement :
	EndToEndFlow | FlowElement;

Processor :
	ProcessorPrototype | ProcessorSubcomponent | ProcessorClassifier;

Memory :
	MemoryClassifier | MemorySubcomponent | MemoryPrototype;

AnnexLibrary :
	DefaultAnnexLibrary;

Flow :
	FlowFeature | FlowImplementation;

ModalElement :
	Subcomponent | ModalPropertyValue | ModalPath | AnnexSubclause | SubprogramCallSequence;

Context :
	Parameter | EventDataPort | FeatureGroup | Subcomponent | SubprogramCall | DataPort;

VirtualBus :
	VirtualBusPrototype | VirtualBusClassifier | VirtualBusSubcomponent;

Thread :
	ThreadClassifier | ThreadPrototype | ThreadSubcomponent;

ComponentTypeRename :
	{ComponentTypeRename} 'componenttyperename'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & category = ComponentCategory &
	renamedComponentType =[ComponentType|IDDT]) '}?';

TriggerPort :
	InternalFeature | Port | AbstractFeature | PortProxy;

Namespace :
	PropertySet | Classifier | GlobalNamespace | RecordType | PackageSection | EnumerationType;

Bus :
	BusClassifier | BusSubcomponent | BusPrototype;

RefinableElement :
	StructuralFeature;

terminal INTEGER returns Integer:
'INTEGER' /*Don't know how to handle INTEGER!*/
;

PropertyValue :
	ReferenceValue | ComputedValue | BooleanLiteral | RangeValue | StringLiteral | ClassifierValue | NumberValue |
	RecordValue | NamedValue;

ListValue :
	{ListValue} 'listvalue''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(ownedListElement += PropertyExpression ( ',' ownedListElement += PropertyExpression)*)?) '}?';

Operation :
	{Operation} 'operation'op = OperationKind '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyExpression += PropertyExpression ( ',' ownedPropertyExpression += PropertyExpression)*)) '}?';

MetaclassReference :
	{MetaclassReference} 'metaclassreference''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?annexName = STRING ? (metaclassName += STRING ( ',' metaclassName += STRING)*)) '}?';

ClassifierValue :
	{ClassifierValue} 'classifiervalue''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?classifier =[Classifier|IDDT]) '}?';

terminal STRING returns String:
'"' ( '\\' . | !('\\' | '"'))* '"' | "'" ( '\\' . | !('\\' | "'"))* "'";

terminal BOOLEAN returns Boolean:
'BOOLEAN' /*Don't know how to handle BOOLEAN!*/
;

ModalPropertyValue :
	{ModalPropertyValue} 'modalpropertyvalue'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?
	(inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?ownedValue = PropertyExpression ) '}?';

Property :
	{Property} 'property'inherit = BOOLEAN emptyListDefault = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?type =[Type|IDDT]?referencedPropertyType =[PropertyType|IDDT]?ownedPropertyType = PropertyType ?defaultValue = PropertyExpression ? (appliesTo += PropertyOwner ( ',' appliesTo += PropertyOwner)*)?) '}?';

Classifier :
	FeatureGroupType | ComponentClassifier;

ComponentPrototypeActual :
	{ComponentPrototypeActual} 'componentprototypeactual'category = ComponentCategory '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(binding += PrototypeBinding ( ',' binding += PrototypeBinding)*)? & subcomponentType =[SubcomponentType|IDDT]) '}?';

FeaturePrototypeActual :
	AccessSpecification | PortSpecification | FeatureGroupPrototypeActual | FeaturePrototypeReference;

Subcomponent :
	DataSubcomponent | BusSubcomponent | SubprogramSubcomponent | ProcessSubcomponent | VirtualBusSubcomponent |
	SubprogramGroupSubcomponent | AbstractSubcomponent | VirtualProcessorSubcomponent | DeviceSubcomponent |
	ProcessorSubcomponent | SystemSubcomponent | ThreadSubcomponent | MemorySubcomponent | ThreadGroupSubcomponent;

Feature :
	Access | DirectedFeature;

Mode :
	{Mode} 'mode'initial = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?) '}?';

ReferenceValue :
	{ReferenceValue} 'referencevalue''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	path = ContainmentPathElement ) '}?';

ArraySizeProperty :
	PropertyConstant;

FlowElement :
	Subcomponent | Connection | FlowSpecification | DataAccess;

FeaturePrototypeBinding :
	{FeaturePrototypeBinding} 'featureprototypebinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?formal =[Prototype|IDDT] actual = FeaturePrototypeActual ) '}?';

FeatureGroupPrototypeBinding :
	{FeatureGroupPrototypeBinding} 'featuregroupprototypebinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & formal =[Prototype|IDDT] & actual = FeatureGroupPrototypeActual ) '}?';

ComponentPrototypeBinding :
	{ComponentPrototypeBinding} 'componentprototypebinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & formal =[Prototype|IDDT] & actual += ComponentPrototypeActual ( ',' actual += ComponentPrototypeActual)*) '}?';

DirectedRelationship :
	Generalization;

BasicProperty :
	RecordField | Property;

ComponentImplementation :
	DataImplementation | VirtualProcessorImplementation | MemoryImplementation | SystemImplementation |
	ProcessImplementation | DeviceImplementation | BehavioredImplementation | VirtualBusImplementation |
	BusImplementation | SubprogramGroupImplementation | ThreadGroupImplementation | ProcessorImplementation;

StructuralFeature :
	InternalFeature | Feature | Subcomponent | ProcessorFeature | FlowFeature | Connection | Prototype;

BusSubcomponent :
	{BusSubcomponent} 'bussubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	busSubcomponentType =[BusSubcomponentType|IDDT]?) '}?';

BusPrototype :
	{BusPrototype} 'busprototype'array = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & refined =[Prototype|IDDT]? &
	constrainingClassifier =[ComponentClassifier|IDDT]?) '}?';

BusClassifier :
	BusType | BusImplementation;

RecordType :
	{RecordType} 'recordtype'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?
	(ownedField += BasicProperty ( ',' ownedField += BasicProperty)*)?) '}?';

EnumerationType :
	UnitsType;

PackageSection :
	PrivatePackageSection | PublicPackageSection;

GlobalNamespace :
	{GlobalNamespace} 'globalnamespace'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?) '}?';

PropertySet :
	{PropertySet} 'propertyset'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?
	(ownedPropertyType += PropertyType ( ',' ownedPropertyType += PropertyType)*)? (ownedProperty += Property ( ',' ownedProperty += Property)*)? (ownedPropertyConstant += PropertyConstant ( ',' ownedPropertyConstant += PropertyConstant)*)? (importedUnit +=[ModelUnit|IDDT] ( ',' importedUnit +=[ModelUnit|IDDT])*)? (ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)?) '}?';

ComponentClassifier :
	ThreadClassifier | ThreadGroupClassifier | MemoryClassifier | DeviceClassifier | ComponentImplementation |
	ProcessorClassifier | AbstractClassifier | SubprogramGroupClassifier | VirtualProcessorClassifier | ComponentType |
	ProcessClassifier | BusClassifier | VirtualBusClassifier | SystemClassifier | DataClassifier | SubprogramClassifier;

FeatureGroupType :
	{FeatureGroupType} 'featuregrouptype' (noPrototypes = BOOLEAN noAnnexes = BOOLEAN noProperties = BOOLEAN )
	name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? &
	(ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? & (ownedPrototype += Prototype
	( ',' ownedPrototype += Prototype)*)? & (ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? & inverse =[FeatureGroupType|IDDT]? & ownedExtension = GroupExtension ? &
	(ownedBusAccess += BusAccess ( ',' ownedBusAccess += BusAccess)*)? & (ownedDataAccess += DataAccess ( ',' ownedDataAccess += DataAccess)*)? & (ownedDataPort += DataPort ( ',' ownedDataPort += DataPort)*)? &
	(ownedEventDataPort += EventDataPort ( ',' ownedEventDataPort += EventDataPort)*)? & (ownedEventPort += EventPort
	( ',' ownedEventPort += EventPort)*)? & (ownedFeatureGroup += FeatureGroup ( ',' ownedFeatureGroup += FeatureGroup)*)? & (ownedParameter += Parameter ( ',' ownedParameter += Parameter)*)? &
	(ownedSubprogramAccess += SubprogramAccess ( ',' ownedSubprogramAccess += SubprogramAccess)*)? &
	(ownedSubprogramGroupAccess += SubprogramGroupAccess ( ',' ownedSubprogramGroupAccess += SubprogramGroupAccess)*)? &
	(ownedAbstractFeature += AbstractFeature ( ',' ownedAbstractFeature += AbstractFeature)*)?) '}?';

AbstractFeature :
	{AbstractFeature} 'abstractfeature' (in = BOOLEAN out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?refined =[Feature|IDDT]?featurePrototype =[FeaturePrototype|IDDT]?abstractFeatureClassifier =[FeatureClassifier|IDDT]?) '}?';

Port :
	EventDataPort | EventPort | DataPort;

InternalFeature :
	EventSource | EventDataSource;

PortProxy :
	{PortProxy} 'portproxy' (in = BOOLEAN & out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?dataClassifier =[DataClassifier|IDDT]?) '}?';

enum ComponentCategory:
abstract | bus | data | device | memory | process | processor | subprogram | subprogramGroup | system | thread | threadGroup | virtualBus | virtualProcessor ;

ComponentType :
	ProcessorType | MemoryType | SystemType | SubprogramType | VirtualBusType | DeviceType | ProcessType | AbstractType |
	ThreadType | BusType | DataType | SubprogramGroupType | ThreadGroupType | VirtualProcessorType;

ThreadSubcomponent :
	{ThreadSubcomponent} 'threadsubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	threadSubcomponentType =[ThreadSubcomponentType|IDDT]?) '}?';

ThreadClassifier :
	ThreadImplementation | ThreadType;

ThreadPrototype :
	{ThreadPrototype} 'threadprototype'array = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & refined =[Prototype|IDDT]? &
	constrainingClassifier =[ComponentClassifier|IDDT]?) '}?';

VirtualBusSubcomponent :
	{VirtualBusSubcomponent} 'virtualbussubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	virtualBusSubcomponentType =[VirtualBusSubcomponentType|IDDT]?) '}?';

VirtualBusClassifier :
	VirtualBusType | VirtualBusImplementation;

VirtualBusPrototype :
	{VirtualBusPrototype} 'virtualbusprototype'array = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & refined =[Prototype|IDDT]? &
	constrainingClassifier =[ComponentClassifier|IDDT]?) '}?';

FeatureGroup :
	{FeatureGroup} 'featuregroup'in = BOOLEAN out = BOOLEAN inverse = BOOLEAN name = IDDT '{' ((ownedComment += Comment
	( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? & refined =[Feature|IDDT]? & featureType =[FeatureType|IDDT]?) '}?';

DataPort :
	{DataPort} 'dataport' (in = BOOLEAN out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?refined =[Feature|IDDT]?dataFeatureClassifier =[DataSubcomponentType|IDDT]?) '}?';

Parameter :
	{Parameter} 'parameter' (in = BOOLEAN out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?refined =[Feature|IDDT]?dataFeatureClassifier =[DataSubcomponentType|IDDT]?) '}?';

SubprogramCall :
	{SubprogramCall} 'subprogramcall'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?calledSubprogram =[CalledSubprogram|IDDT]?context =[CallContext|IDDT]?) '}?';

EventDataPort :
	{EventDataPort} 'eventdataport' (in = BOOLEAN out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? & refined =[Feature|IDDT]? & dataFeatureClassifier =[DataSubcomponentType|IDDT]?)
	'}?';

SubprogramGroupSubcomponent :
	{SubprogramGroupSubcomponent} 'subprogramgroupsubcomponent'allModes = BOOLEAN name = IDDT '{'
	((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?subprogramGroupSubcomponentType =[SubprogramGroupSubcomponentType|IDDT]?) '}?';

ProcessSubcomponent :
	{ProcessSubcomponent} 'processsubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	processSubcomponentType =[ProcessSubcomponentType|IDDT]?) '}?';

SystemSubcomponent :
	{SystemSubcomponent} 'systemsubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	systemSubcomponentType =[SystemSubcomponentType|IDDT]?) '}?';

ProcessorSubcomponent :
	{ProcessorSubcomponent} 'processorsubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?processorSubcomponentType =[ProcessorSubcomponentType|IDDT]?) '}?';

MemorySubcomponent :
	{MemorySubcomponent} 'memorysubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	memorySubcomponentType =[MemorySubcomponentType|IDDT]?) '}?';

DataSubcomponent :
	{DataSubcomponent} 'datasubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	dataSubcomponentType =[DataSubcomponentType|IDDT]?) '}?';

SubprogramSubcomponent :
	{SubprogramSubcomponent} 'subprogramsubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	subprogramSubcomponentType =[SubprogramSubcomponentType|IDDT]?) '}?';

ThreadGroupSubcomponent :
	{ThreadGroupSubcomponent} 'threadgroupsubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	threadGroupSubcomponentType =[ThreadGroupSubcomponentType|IDDT]?) '}?';

DeviceSubcomponent :
	{DeviceSubcomponent} 'devicesubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
