grammar opL

import "http://aadl.info/AADL/2.0"
import "http://www.eclipse.org/emf/2002/Ecore" as ecore


Element :
	PrototypeBinding | ArrayRange | ArrayDimension | Comment | FlowEnd | PropertyExpression | EndToEndFlowSegment |
	ModeBinding | BasicPropertyAssociation | ContainedNamedElement | FlowSegment | ArrayableElement |
	ModeTransitionTrigger | NamedElement | PropertyAssociation | ContainmentPathElement |
	ComponentImplementationReference | Relationship | PropertyOwner | ArraySize | ConnectedElement | NumericRange;

PrototypeBinding :
	FeaturePrototypeBinding | ComponentPrototypeBinding | FeatureGroupPrototypeBinding;

ModeTransitionTrigger :
	{ModeTransitionTrigger} 'ModeTransitionTrigger''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	context =[Context|IDDT]? & triggerPort =[TriggerPort|IDDT]) '}?';

NamedElement :
	ConnectionEnd | Process | EnumerationLiteral | Memory | EndToEndFlowElement | Data | ModelUnit | TriggerPort |
	ComponentTypeRename | Abstract | VirtualProcessor | Type | Namespace | ClassifierFeature | Bus | Device |
	PackageRename | ThreadGroup | VirtualBus | FeatureGroupTypeRename | Thread | Processor | System | SubprogramGroup |
	RefinableElement | AnnexLibrary | ModalElement | Subprogram | Context | TypedElement | Flow;

FlowEnd :
	{FlowEnd} 'FlowEnd''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?context =[Context|IDDT]?feature =[Feature|IDDT]) '}?';

ComponentImplementationReference :
	{ComponentImplementationReference} 'ComponentImplementationReference''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & implementation =[ComponentImplementation|IDDT] &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?) '}?';

ArrayRange :
	{ArrayRange} 'ArrayRange''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?lowerBound = INTEGER upperBound = INTEGER ) '}?';

PropertyOwner :
	MetaclassReference | ClassifierValue;

PropertyAssociation :
	{PropertyAssociation} 'PropertyAssociation' (append = BOOLEAN & constant = BOOLEAN ) '{' ((ownedComment += Comment
	( ',' ownedComment += Comment)*)?property =[Property|IDDT] (appliesTo += ContainedNamedElement ( ',' appliesTo += ContainedNamedElement)*)? (inBinding +=[Classifier|IDDT] ( ',' inBinding +=[Classifier|IDDT])*)?
	(ownedValue += ModalPropertyValue ( ',' ownedValue += ModalPropertyValue)*)) '}?';

PropertyExpression :
	PropertyValue | Operation | ListValue;

NumericRange :
	{NumericRange} 'NumericRange''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?upperBound = PropertyExpression lowerBound = PropertyExpression ) '}?';

Relationship :
	DirectedRelationship;

Comment :
	{Comment} 'Comment''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & body = STRING ?) '}?';

ArrayableElement :
	Feature | ComponentPrototypeActual | FeaturePrototypeActual | Subcomponent;

ContainmentPathElement :
	{ContainmentPathElement} 'ContainmentPathElement''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(arrayRange += ArrayRange ( ',' arrayRange += ArrayRange)*)? & namedElement =[NamedElement|IDDT] &
	annexName = STRING ? & path = ContainmentPathElement ?) '}?';

FlowSegment :
	{FlowSegment} 'FlowSegment''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?flowElement =[FlowElement|IDDT] context =[Context|IDDT]?) '}?';

ArrayDimension :
	{ArrayDimension} 'ArrayDimension''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?size = ArraySize ?)
	'}?';

EndToEndFlowSegment :
	{EndToEndFlowSegment} 'EndToEndFlowSegment''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?flowElement =[EndToEndFlowElement|IDDT] context =[Context|IDDT]?) '}?';

ModeBinding :
	{ModeBinding} 'ModeBinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?parentMode =[Mode|IDDT]
	derivedMode =[Mode|IDDT]?) '}?';

ConnectedElement :
	{ConnectedElement} 'ConnectedElement''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	context =[Context|IDDT]? & connectionEnd =[ConnectionEnd|IDDT] & next = ConnectedElement ?) '}?';

BasicPropertyAssociation :
	{BasicPropertyAssociation} 'BasicPropertyAssociation''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?property =[BasicProperty|IDDT] ownedValue = PropertyExpression ) '}?';

ContainedNamedElement :
	ReferenceValue;

ArraySize :
	{ArraySize} 'ArraySize''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?size = INTEGER sizeProperty =[ArraySizeProperty|IDDT]?) '}?';

ArraySizeProperty :
	PropertyConstant;

terminal INTEGER returns Integer:
'INTEGER' /*Don't know how to handle INTEGER!*/
;

ReferenceValue :
	{ReferenceValue} 'ReferenceValue''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	path = ContainmentPathElement ) '}?';

BasicProperty :
	Property | RecordField;

Context :
	FeatureGroup | EventDataPort | Parameter | DataPort | Subcomponent | SubprogramCall;

ConnectionEnd :
	AccessConnectionEnd | FeatureConnectionEnd | ParameterConnectionEnd | FeatureGroupConnectionEnd | PortConnectionEnd;

Mode :
	{Mode} '!Mode'initial = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?) '}?';

EndToEndFlowElement :
	FlowElement | EndToEndFlow;

FlowElement :
	Connection | FlowSpecification | DataAccess | Subcomponent;

terminal STRING returns String:
'"' ( '\\' . | !('\\' | '"'))* '"' | "'" ( '\\' . | !('\\' | "'"))* "'";

FeaturePrototypeActual :
	FeaturePrototypeReference | AccessSpecification | FeatureGroupPrototypeActual | PortSpecification;

ComponentPrototypeActual :
	{ComponentPrototypeActual} 'ComponentPrototypeActual'category = ComponentCategory '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(binding += PrototypeBinding ( ',' binding += PrototypeBinding)*)?subcomponentType =[SubcomponentType|IDDT]) '}?';

Subcomponent :
	SubprogramSubcomponent | VirtualProcessorSubcomponent | ThreadGroupSubcomponent | SystemSubcomponent |
	BusSubcomponent | ProcessorSubcomponent | MemorySubcomponent | ProcessSubcomponent | VirtualBusSubcomponent |
	AbstractSubcomponent | DataSubcomponent | DeviceSubcomponent | SubprogramGroupSubcomponent | ThreadSubcomponent;

Feature :
	Access | DirectedFeature;

DirectedRelationship :
	Generalization;

Operation :
	{Operation} 'Operation'op = OperationKind '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	ownedPropertyExpression += PropertyExpression ( ',' ownedPropertyExpression += PropertyExpression)*) '}?';

ListValue :
	{ListValue} 'ListValue''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(ownedListElement += PropertyExpression ( ',' ownedListElement += PropertyExpression)*)?) '}?';

PropertyValue :
	ComputedValue | RecordValue | ClassifierValue | NamedValue | ReferenceValue | BooleanLiteral | NumberValue |
	RangeValue | StringLiteral;

terminal BOOLEAN returns Boolean:
'BOOLEAN' /*Don't know how to handle BOOLEAN!*/
;

ModalPropertyValue :
	{ModalPropertyValue} '!ModalPropertyValue'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	& (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? &
	(inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? & ownedValue = PropertyExpression ) '}?';

Property :
	{Property} '!Property' (inherit = BOOLEAN emptyListDefault = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment
	( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?type =[Type|IDDT]?referencedPropertyType =[PropertyType|IDDT]?ownedPropertyType = PropertyType ?defaultValue = PropertyExpression ? (appliesTo += PropertyOwner ( ',' appliesTo += PropertyOwner)*)?) '}?';

Classifier :
	FeatureGroupType | ComponentClassifier;

MetaclassReference :
	{MetaclassReference} 'MetaclassReference''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	annexName = STRING ? & metaclassName += STRING ( ',' metaclassName += STRING)*) '}?';

ClassifierValue :
	{ClassifierValue} 'ClassifierValue''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	classifier =[Classifier|IDDT]) '}?';

ComponentImplementation :
	BusImplementation | BehavioredImplementation | SubprogramGroupImplementation | SystemImplementation |
	ProcessorImplementation | ThreadGroupImplementation | VirtualBusImplementation | DataImplementation |
	DeviceImplementation | MemoryImplementation | ProcessImplementation | VirtualProcessorImplementation;

Memory :
	MemoryPrototype | MemoryClassifier | MemorySubcomponent;

Data :
	DataSubcomponent | DataPrototype | DataClassifier;

SubprogramGroup :
	SubprogramGroupClassifier | SubprogramGroupPrototype | SubprogramGroupSubcomponent;

FeatureGroupTypeRename :
	{FeatureGroupTypeRename} '!FeatureGroupTypeRename'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & renamedFeatureGroupType =[FeatureGroupType|IDDT]) '}?';

ClassifierFeature :
	ModeFeature | FlowImplementation | BehavioralFeature | StructuralFeature;

Subprogram :
	SubprogramSubcomponent | SubprogramClassifier | SubprogramPrototype;

PackageRename :
	{PackageRename} '!PackageRename'renameAll = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?renamedPackage =[AadlPackage|IDDT]) '}?';

Process :
	ProcessSubcomponent | ProcessPrototype | ProcessClassifier;

TypedElement :
	PropertyConstant | BasicProperty;

EnumerationLiteral :
	UnitLiteral;

RefinableElement :
	StructuralFeature;

Namespace :
	EnumerationType | Classifier | GlobalNamespace | RecordType | PackageSection | PropertySet;

ThreadGroup :
	ThreadGroupSubcomponent | ThreadGroupPrototype | ThreadGroupClassifier;

Device :
	DevicePrototype | DeviceSubcomponent | DeviceClassifier;

Processor :
	ProcessorPrototype | ProcessorClassifier | ProcessorSubcomponent;

ModelUnit :
	AadlPackage | PropertySet;

VirtualProcessor :
	VirtualProcessorSubcomponent | VirtualProcessorClassifier | VirtualProcessorPrototype;

Thread :
	ThreadClassifier | ThreadPrototype | ThreadSubcomponent;

TriggerPort :
	InternalFeature | Port | PortProxy | AbstractFeature;

VirtualBus :
	VirtualBusSubcomponent | VirtualBusPrototype | VirtualBusClassifier;

Bus :
	BusClassifier | BusSubcomponent | BusPrototype;

ComponentTypeRename :
	{ComponentTypeRename} '!ComponentTypeRename'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?category = ComponentCategory renamedComponentType =[ComponentType|IDDT]) '}?';

Abstract :
	AbstractClassifier | AbstractPrototype | AbstractSubcomponent;

AnnexLibrary :
	DefaultAnnexLibrary;

ModalElement :
	ModalPath | AnnexSubclause | SubprogramCallSequence | Subcomponent | ModalPropertyValue;

Flow :
	FlowImplementation | FlowFeature;

Type :
	PropertyType | Classifier | SubcomponentType;

System :
	SystemPrototype | SystemSubcomponent | SystemClassifier;

FeaturePrototypeBinding :
	{FeaturePrototypeBinding} 'FeaturePrototypeBinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	formal =[Prototype|IDDT] & actual = FeaturePrototypeActual ) '}?';

FeatureGroupPrototypeBinding :
	{FeatureGroupPrototypeBinding} 'FeatureGroupPrototypeBinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & formal =[Prototype|IDDT] & actual = FeatureGroupPrototypeActual ) '}?';

ComponentPrototypeBinding :
	{ComponentPrototypeBinding} 'ComponentPrototypeBinding''{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & formal =[Prototype|IDDT] & actual += ComponentPrototypeActual ( ',' actual += ComponentPrototypeActual)*) '}?';

PropertyConstant :
	{PropertyConstant} '!PropertyConstant'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?type =[Type|IDDT]?referencedPropertyType =[PropertyType|IDDT]?ownedPropertyType = PropertyType ?constantValue = PropertyExpression ?) '}?';

RecordField :
	{RecordField} '!RecordField'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? &
	type =[Type|IDDT]? & referencedPropertyType =[PropertyType|IDDT]? & ownedPropertyType = PropertyType ?) '}?';

PropertyType :
	NonListType | ListType;

SubcomponentType :
	VirtualProcessorSubcomponentType | VirtualBusSubcomponentType | SubprogramGroupSubcomponentType | ComponentClassifier | AbstractSubcomponentType | MemorySubcomponentType | DeviceSubcomponentType | ThreadSubcomponentType |
	BusSubcomponentType | DataSubcomponentType | ProcessorSubcomponentType | SystemSubcomponentType |
	ProcessSubcomponentType | ComponentPrototype | SubprogramSubcomponentType | ThreadGroupSubcomponentType;

FeatureGroupType :
	{FeatureGroupType} '!FeatureGroupType' (noPrototypes = BOOLEAN noAnnexes = BOOLEAN noProperties = BOOLEAN )
	name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? &
	(ownedAnnexSubclause += AnnexSubclause ( ',' ownedAnnexSubclause += AnnexSubclause)*)? & (ownedPrototype += Prototype
	( ',' ownedPrototype += Prototype)*)? & (ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? & inverse =[FeatureGroupType|IDDT]? & ownedExtension = GroupExtension ? &
	(ownedBusAccess += BusAccess ( ',' ownedBusAccess += BusAccess)*)? & (ownedDataAccess += DataAccess ( ',' ownedDataAccess += DataAccess)*)? & (ownedDataPort += DataPort ( ',' ownedDataPort += DataPort)*)? &
	(ownedEventDataPort += EventDataPort ( ',' ownedEventDataPort += EventDataPort)*)? & (ownedEventPort += EventPort
	( ',' ownedEventPort += EventPort)*)? & (ownedFeatureGroup += FeatureGroup ( ',' ownedFeatureGroup += FeatureGroup)*)? & (ownedParameter += Parameter ( ',' ownedParameter += Parameter)*)? &
	(ownedSubprogramAccess += SubprogramAccess ( ',' ownedSubprogramAccess += SubprogramAccess)*)? &
	(ownedSubprogramGroupAccess += SubprogramGroupAccess ( ',' ownedSubprogramGroupAccess += SubprogramGroupAccess)*)? &
	(ownedAbstractFeature += AbstractFeature ( ',' ownedAbstractFeature += AbstractFeature)*)?) '}?';

ComponentClassifier :
	VirtualProcessorClassifier | SubprogramClassifier | ComponentType | ThreadClassifier | ComponentImplementation |
	VirtualBusClassifier | SystemClassifier | DeviceClassifier | MemoryClassifier | ProcessClassifier |
	AbstractClassifier | SubprogramGroupClassifier | BusClassifier | ProcessorClassifier | ThreadGroupClassifier |
	DataClassifier;

Parameter :
	{Parameter} '!Parameter' (in = BOOLEAN & out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?refined =[Feature|IDDT]?dataFeatureClassifier =[DataSubcomponentType|IDDT]?) '}?';

DataPort :
	{DataPort} '!DataPort' (in = BOOLEAN & out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? & refined =[Feature|IDDT]? & dataFeatureClassifier =[DataSubcomponentType|IDDT]?)
	'}?';

FeatureGroup :
	{FeatureGroup} '!FeatureGroup'in = BOOLEAN out = BOOLEAN inverse = BOOLEAN name = IDDT '{' ((ownedComment += Comment
	( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?refined =[Feature|IDDT]?featureType =[FeatureType|IDDT]?) '}?';

EventDataPort :
	{EventDataPort} '!EventDataPort' (in = BOOLEAN & out = BOOLEAN ) name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?refined =[Feature|IDDT]?dataFeatureClassifier =[DataSubcomponentType|IDDT]?) '}?';

SubprogramCall :
	{SubprogramCall} '!SubprogramCall'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)?
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)?calledSubprogram =[CalledSubprogram|IDDT]?context =[CallContext|IDDT]?) '}?';

SubprogramGroupSubcomponent :
	{SubprogramGroupSubcomponent} '!SubprogramGroupSubcomponent'allModes = BOOLEAN name = IDDT '{'
	((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?subprogramGroupSubcomponentType =[SubprogramGroupSubcomponentType|IDDT]?) '}?';

ProcessorSubcomponent :
	{ProcessorSubcomponent} '!ProcessorSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	processorSubcomponentType =[ProcessorSubcomponentType|IDDT]?) '}?';

SystemSubcomponent :
	{SystemSubcomponent} '!SystemSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?systemSubcomponentType =[SystemSubcomponentType|IDDT]?) '}?';

SubprogramSubcomponent :
	{SubprogramSubcomponent} '!SubprogramSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	subprogramSubcomponentType =[SubprogramSubcomponentType|IDDT]?) '}?';

BusSubcomponent :
	{BusSubcomponent} '!BusSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	busSubcomponentType =[BusSubcomponentType|IDDT]?) '}?';

ProcessSubcomponent :
	{ProcessSubcomponent} '!ProcessSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?processSubcomponentType =[ProcessSubcomponentType|IDDT]?) '}?';

ThreadGroupSubcomponent :
	{ThreadGroupSubcomponent} '!ThreadGroupSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment
	( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?threadGroupSubcomponentType =[ThreadGroupSubcomponentType|IDDT]?) '}?';

DataSubcomponent :
	{DataSubcomponent} '!DataSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	dataSubcomponentType =[DataSubcomponentType|IDDT]?) '}?';

ThreadSubcomponent :
	{ThreadSubcomponent} '!ThreadSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?threadSubcomponentType =[ThreadSubcomponentType|IDDT]?) '}?';

AbstractSubcomponent :
	{AbstractSubcomponent} '!AbstractSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	abstractSubcomponentType =[AbstractSubcomponentType|IDDT]?) '}?';

DeviceSubcomponent :
	{DeviceSubcomponent} '!DeviceSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	deviceSubcomponentType =[DeviceSubcomponentType|IDDT]?) '}?';

VirtualBusSubcomponent :
	{VirtualBusSubcomponent} '!VirtualBusSubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?virtualBusSubcomponentType =[VirtualBusSubcomponentType|IDDT]?) '}?';

VirtualProcessorSubcomponent :
	{VirtualProcessorSubcomponent} '!VirtualProcessorSubcomponent'allModes = BOOLEAN name = IDDT '{'
	((ownedComment += Comment ( ',' ownedComment += Comment)*)? (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)?
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)?
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)?
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)?
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)?refined =[Subcomponent|IDDT]?virtualProcessorSubcomponentType =[VirtualProcessorSubcomponentType|IDDT]?) '}?';

MemorySubcomponent :
	{MemorySubcomponent} '!MemorySubcomponent'allModes = BOOLEAN name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? & (ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? & (inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? &
	(arrayDimension += ArrayDimension ( ',' arrayDimension += ArrayDimension)*)? &
	(ownedPrototypeBinding += PrototypeBinding ( ',' ownedPrototypeBinding += PrototypeBinding)*)? &
	(ownedModeBinding += ModeBinding ( ',' ownedModeBinding += ModeBinding)*)? &
	(implementationReference += ComponentImplementationReference ( ',' implementationReference += ComponentImplementationReference)*)? & refined =[Subcomponent|IDDT]? &
	memorySubcomponentType =[MemorySubcomponentType|IDDT]?) '}?';

ParameterConnectionEnd :
	EventDataPort | DataSubcomponent | Parameter | DataPort | DataAccess;

AccessConnectionEnd :
	SubprogramSubcomponent | VirtualBusSubcomponent | Access | DataSubcomponent | SubprogramGroupSubcomponent |
	BusSubcomponent | SubprogramProxy;

FeatureGroupConnectionEnd :
	FeatureGroup;

PortConnectionEnd :
	InternalFeature | DataSubcomponent | Port | DataAccess | PortProxy;

FeatureConnectionEnd :
	Feature | InternalFeature | PortProxy;

EndToEndFlow :
	{EndToEndFlow} '!EndToEndFlow'name = IDDT '{' ((ownedComment += Comment ( ',' ownedComment += Comment)*)? &
	(ownedPropertyAssociation += PropertyAssociation ( ',' ownedPropertyAssociation += PropertyAssociation)*)? &
	(inMode +=[Mode|IDDT] ( ',' inMode +=[Mode|IDDT])*)? & (inModeOrTransition +=[ModeFeature|IDDT] ( ',' inModeOrTransition +=[ModeFeature|IDDT])*)? & refined =[EndToEndFlow|IDDT]? &
